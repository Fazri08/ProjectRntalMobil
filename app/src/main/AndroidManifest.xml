        <activity android:name=".Admin.Activity.MenuActivity"/>
        <activity android:name=".Admin.Activity.ListOrder"/>

<!--        Activity Customer-->
        <activity android:name=".Customer.Activity.MainBottom"/>
        <activity android:name=".Customer.Activity.Cart"/>
        <activity android:name=".Customer.Activity.ConfirmOrder"/>
